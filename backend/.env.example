# Asterisk Admin - Configurações

# App
APP_NAME=Asterisk Admin
APP_VERSION=1.0.0
DEBUG=True

# Database
DATABASE_URL=postgresql+asyncpg://asterisk:asterisk@localhost:5432/asterisk_admin
DATABASE_SYNC_URL=postgresql://asterisk:asterisk@localhost:5432/asterisk_admin

# Security - ALTERE EM PRODUÇÃO!
SECRET_KEY=sua-chave-secreta-muito-segura-aqui-mude-em-producao
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=480

# Asterisk AMI
AMI_HOST=127.0.0.1
AMI_PORT=5038
AMI_USERNAME=admin
AMI_SECRET=admin

# Redis (opcional)
REDIS_URL=redis://localhost:6379/0

# Asterisk Paths
ASTERISK_CONFIG_PATH=/etc/asterisk
ASTERISK_SPOOL_PATH=/var/spool/asterisk/outgoing
